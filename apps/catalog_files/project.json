{
  "name": "catalog_files",
  "root": "apps/catalog_files",
  "projectType": "application",
  "sourceRoot": "apps/catalog_files/src",
  "targets": {
    "execute": {
      "executor": "@nx/exec:exec",
      "options": {
        "command": "python src/main.py"
      }
    },
    "build": {
      "executor": "@nxlv/python:poetry-build",
      "outputs": [
        "{workspaceRoot}/dist/apps/catalog_files"
      ],
      "options": {}
    },
    "lint": {
      "executor": "@nxlv/python:flake8",
      "outputs": [
        "{workspaceRoot}/dist/pylint/apps/catalog_files"
      ],
      "options": {
        "outputFile": "dist/pylint/apps/catalog_files/report.txt"
      }
    },
    "test": {
      "executor": "@nxlv/python:pytest",
      "outputs": [
        "{workspaceRoot}/coverage/apps/catalog_files"
      ],
      "options": {
        "codeCoverage": true,
        "junitOutput": true,
        "outputFile": "coverage/apps/catalog_files/report.xml"
      }
    },
    "install": {
      "executor": "@nx/exec:exec",
      "options": {
        "command": "poetry install"
      }
    },
    "add": {
      "executor": "@nxlv/python:poetry-add"
    },
    "update-lock": {
      "executor": "@nxlv/python:poetry-update"
    }
  },
  "tags": ["python", "app"]
}